# QR Thermal Label App Environment Variables
# Copy this file to .env and fill in the values

# Google Apps Script Web App URL (Legacy)
VITE_GAS_WEBAPP_URL=your_google_apps_script_url_here

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:xxxxx

# Feature Flag: Use Firebase instead of Google Sheets
# Set to 'true' to use Firebase, 'false' to use Google Sheets
VITE_USE_FIREBASE=false
